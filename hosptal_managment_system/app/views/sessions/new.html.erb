<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_for(@employee) do |f| %>
<%= f.label :Name %>
<%= f.text_field :name %>
<br>
<%= f.label :Email %>
<%= f.text_field :email %>
<br>
<%= f.label :Password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation, "Confirmation" %>
<%= f.password_field :password_confirmation %>
<br>

<%= f.submit :"Create account" %>
<% end %>